{
  "expo": {
    "name": "Tubex Dubai",
    "slug": "tubex-dubai",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A1628"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tubexdubai.app",
      "buildNumber": "2",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app does not require location access.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "usesAppleSignIn": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#0A1628"
      },
      "package": "com.tubexdubai.app",
      "versionCode": 2,
      "permissions": [
        "RECEIVE_BOOT_COMPLETED"
      ]
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/logo.png",
          "color": "#4A90E2",
          "sounds": [],
          "mode": "production"
        }
      ]
    ],
    "web": {
      "favicon": "./assets/logo.png"
    },
    "extra": {
      "eas": {
        "projectId": "133bee5a-79c5-4bcc-b3e9-a838a892bcc7"
      }
    }
  }
}
